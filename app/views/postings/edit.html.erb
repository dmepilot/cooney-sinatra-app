<h1>Insert the form to edit an instance of your model here.</h1>

<form action="/postings/<%=@posting.id%>" method="post">
<input type="hidden" name="_method" value="PATCH">
<label for="posting[title]">Title:</label>
<input type="text" name="posting[title]" value="<%=@posting.title%>"><br>
<label for="posting[location]">Location:</label>
<input type="text" name="posting[location]" value="<%=@posting.location%>"><br>
<label for="posting[details]">Details:</label><br>
<textarea name="posting[details]" rows="6" cols="80"><%=@posting.details%> </textarea><br>

<button type="submit" class="button-2">SUBMIT</button>
</form>
